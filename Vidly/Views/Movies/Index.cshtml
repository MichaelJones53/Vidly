@using Vidly.Models
@model Vidly.Models.Movie[]
@{
    ViewBag.Title = "Movies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1>Movies</h1>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="row">Movie</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var movie in Model){
        <tr class="table-secondary" scope ="row">
        
        <td>@movie.Name</td>
           
        
        </tr>
    }

    </tbody>

</table>


