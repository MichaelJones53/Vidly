

@model Vidly.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@Model.Name</h1>

<ul>
    <li>
        Membership Type: @Model.MembershipType.MembershipTypeAsString
    </li>
    @if (Model.BirthDate != null)
    {
        <li>Birthdate: @Model.BirthDate</li>
    }
</ul>

